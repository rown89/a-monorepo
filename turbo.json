{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build:next": {
			"env": [],
			"dependsOn": [],
			"outputs": ["dist/**", ".next/**", "!.next/cache/**"]
		},
		"lint": {},
